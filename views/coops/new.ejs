<% layout('layouts/boilerplate') %> 
<form action="/coops" method="POST">
    <div>
        <label for="name">Name</label>
        <input type="text" id="name" name="coop[name]">

        <label for="image">Image</label>
        <input type="text" id="image" name="coop[image]">

        <label for="description">Description</label>
        <input type="text" id="description" name="coop[description]">

        <button>Submit</button>
    </div>
</form>

<div>
    <a href="/coops">All Coops</a>
</div>
